{
  "manifest_version": 3,
  "name": "Fomo Player extension",
  "description": "Fomo Player Beatport extension",
  "version": "0.0.2",
  "options_page": "options.html",
  "background": {
    "service_worker": "service_worker.bundle.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Sync to Multi Store Player"
  },
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": []
    }
  ],
  "permissions": ["declarativeContent", "activeTab", "tabs", "storage", "identity", "scripting"],
  "host_permissions": ["https://*/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
