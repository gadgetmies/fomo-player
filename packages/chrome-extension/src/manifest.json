{
  "manifest_version": 2,
  "name": "Multi Store Player Beatport extension",
  "version": "0.0.2",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Sync to Multi Store Player",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["declarativeContent", "activeTab", "tabs", "storage", "identity", "https://*/"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
